<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/colorPrimary"
    tools:context=".administratorSearchUsers.AdministratorSearchUsersActivity">

    <!--AppBar y Toolbar-->
    <include layout="@layout/toolbar"></include>


    <TextView
        android:layout_width="284dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="180dp"
        android:gravity="center_vertical"
        android:text="@string/insert_DNI_name_text_label"
        android:textColor="?attr/colorPrimaryDark"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/user_name_edit_text"
        android:layout_width="240dp"
        android:layout_height="39dp"
        android:layout_marginLeft="61dp"
        android:layout_marginTop="220dp"
        android:layout_marginRight="237dp"
        android:layout_weight="1"
        android:background="?attr/colorPrimaryDarkTrans"

        android:ems="10"
        android:hint="@string/insert_DNI_name_text_input"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColor="?attr/colorPrimary"
        android:textColorHint="?attr/colorAccent" />

    <Button
        android:id="@+id/buttonUserSearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="135dp"
        android:layout_marginTop="500dp"
        android:backgroundTint="?attr/colorPrimaryDark"
        android:text="@string/search"
        android:textColor="?attr/colorAccent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="106dp"
        android:layout_height="116dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="320dp"
        app:srcCompat="@drawable/logo"
        android:tint="?attr/colorPrimaryDark"
        android:contentDescription="TODO" />


</android.support.design.widget.CoordinatorLayout>