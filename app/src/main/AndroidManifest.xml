    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".App.AppMediator"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SignIn.SignInActivity"></activity>

        <activity android:name=".SignUp.SignUpActivity"></activity>

        <activity android:name=".agendaAdmin.AgendaAdminActivity"></activity>
        <activity android:name=".AnimalCliente.AnimalDetailActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity android:name=".AnimalesCliente.AnimalesClienteListActivity"></activity>
    </application>

</manifest>